  {
   "cell_type": "code",
   "execution_count": 38,
   "id": "554a6834-174c-4e72-be88-2dfe1ca1d441",
   "metadata": {},
   "outputs": [],
   "source": [
    "def visualize_attention_v3(text, model, tokenizer):\n",
    "    # 1. 预处理：移除标点（保留原始文本分词）\n",
    "    clean_text = ''.join([c for c in text if c not in [\"，\", \"。\", \"！\", \"？\"]])\n",
    "    inputs = tokenizer(clean_text, return_tensors=\"pt\")\n",
    "    \n",
    "    # 2. 获取注意力权重（限制层和头）\n",
    "    with torch.no_grad():\n",
    "        outputs = model(**inputs, output_attentions=True)\n",
    "    \n",
    "    # 3. 注意力矩阵优化处理\n",
    "    attentions = torch.stack(outputs.attentions[4:8])  # 第5-8层\n",
    "    median_attn = attentions.median(dim=0).values      # 中位数抗噪\n",
    "    head_weights = torch.linspace(0.8, 1.2, attentions.shape[1])  # 头权重衰减\n",
    "    weighted_attn = (median_attn * head_weights.view(-1,1,1,1)).mean(dim=(0,1))\n",
    "    \n",
    "    # 4. 动态归一化（排除对角线）\n",
    "    np_attn = weighted_attn.numpy()\n",
    "    np_attn *= 1 - np.eye(np_attn.shape[0])  # 消除自注意力\n",
    "    normalized_attn = (np_attn - np_attn.min()) / (np_attn.max() - np_attn.min())\n",
    "    \n",
    "    # 5. 可视化优化\n",
    "    tokens = [t.replace(\"##\",\"\") for t in tokenizer.convert_ids_to_tokens(inputs[\"input_ids\"][0])]\n",
    "    plt.figure(figsize=(14,10))\n",
    "    sns.heatmap(\n",
    "        normalized_attn,\n",
    "        xticklabels=tokens,\n",
    "        yticklabels=tokens,\n",
    "        cmap=\"YlOrRd\",  # 改用更醒目的颜色\n",
    "        vmin=0.1,       # 过滤低权重噪声\n",
    "        vmax=0.8,       # 防止最高值主导色阶\n",
    "        center=0.4,\n",
    "        square=True,\n",
    "        linewidths=0.3,\n",
    "        cbar_kws={'label': '归一化注意力'}\n",
    "    )\n",
    "    plt.title(f\"语义注意力热力图: {clean_text[:20]}\", pad=20)\n",
    "    plt.xticks(rotation=45, ha='right', fontsize=11)\n",
    "    plt.yticks(fontsize=11)\n",
    "    plt.tight_layout()\n",
    "    plt.show()"
   ]
  },
